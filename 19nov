package mysqldataBaseConnection;

public class inheritance
{
	public static void main(String args[])
	{
		nenu ne=new nenu();
		nana na=new nana();
		abba ab=new abba();
		ne.oldbike();
		
	}
}
class abba
{
	void ezidi()
	{
		System.out.println("Chana Patha bandi ezidi");
	}
	
	void oldbike()
	{
		System.out.println("Chana Patha bandi ezidi");
	}
	private final void setup()
	{
		System.out.println("10kg bangaram");
	}
	private final void setupland()
	{
		System.out.println("10cents bhumi");
	}
}

class nana extends abba
	{
	void ct100()
	{
		System.out.println("patha bandi ct100");
	}
	
	void oldbike()
	{
		super.oldbike();
		System.out.println("Patha bandi ct100");
	}
}

class nenu extends nana
{
	void hornet()
	{
		System.out.println("kotha bandi hornet");
	}
}

class op
{
	void showop()
	{
		nenu ne=new nenu();
		ne.oldbike();
		nana na=new nana();
		na.setup();         //line 64,65 errors ndukante paina private final vadina kabati
		na.setupland();			// super,final keywords
		
	}
}




